{
    "version": "0.0.3",
    "name": "ids-chrome-extension",
    "background": {
        "matches": ["http://127.0.0.1:8887/"],
        "scripts": ["background.bundle.js"]
    },
    "content_scripts": [
        {
            "matches": ["http://127.0.0.1:8887/"],
            "js": ["contentScript.bundle.js"],
            "all_frames": true
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}

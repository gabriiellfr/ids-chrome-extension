{
    "version": "0.1",
    "name": "ids-chrome-extension",
    "background": {
        "scripts": ["js/plugins/jquery.min.js", "js/background.js"]
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["js/plugins/jquery.min.js", "js/contentScript.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
